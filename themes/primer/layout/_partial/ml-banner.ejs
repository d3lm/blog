<header class="ml-banner">
  <div class="container">
    <hgroup>
      <a href="<%- url_for() %>" title="<%= config.title %>">
        <img class="ml-logo" src="/images/machinelabs_logo_white.svg" alt="MachineLabs logo">
        <h2 class="ml-tagline">Machine Learning for everyone</h2>
      </a>
    </hgroup>
      <nav class="ml-nav" role="navigation">
        <% for (var nav in theme.menus) { %>
              <%
                var title = theme.menus[nav].name;
                var href = theme.menus[nav].link;
                var link = "^" + href;  
                var target = theme.menus[nav].target;
              %>
              <a href="<%- url_for(href) %>" <%if(target){%>target="<%=target%>"<%}%> class="<% if (page && page.path.match(link)) { %> selected <% } %> hvr-underline-from-center" title="<%= title %>"><%= title %></a>
        <% } %>
      </nav>
  </div>
</header>
